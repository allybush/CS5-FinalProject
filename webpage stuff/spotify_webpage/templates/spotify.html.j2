<div class="card row justify-content-center align-items-center" >
    <div class="card-body">

        <h5 class="card-title">Search For a Song</h5>
        <form action="/" method="POST">
            <div class="input-group">
            <div class="form-outline">
                <input placeholder="Search Song/Artist" name="search" type="search" id="form1" class="form-control" />
            </div>
            <input type="submit" class="btn btn-primary">
            </input>
            </div>
        </form>

        {% for result in results %}

            <ul id="myUL">
              {% if result['preview_url'] %}
                <li><a href = '/' onclick="getSongData('{{ result['preview_url'] }}')">{{ result['name'] }}</a></li>
              {% endif %}
            </ul>
        {% endfor %}




    </div>
</div>
